{
    "manifest_version" : 3,
    "name" : "Project DIH",
    "version": "1.0.0",
    "icons": {
        "16": "iconDIH.png"
    },
    "background": {
        "service_worker": "service-worker.js",
        "type": "module"
    },
    "action":{
        "default_popup": "index.html"
    },
    "permissions": [
        "scripting",
        "storage"
    ],
    "host_permissions":[
        "http://*/*",
        "https://*/*"
    ]
}