{
    "manifest_version" : 3,
    "name" : "Project DIH",
    "version": "1.0.0",
    "icons": {
        "16": "iconDIH.png"
    },
    "action":{
        "default_popup": "index.html"
    },
    "permissions": [
        "scripting"
    ],
    "host_permissions":[
        "http://*/*",
        "https://*/*"
    ],
    "content_scripts":{
        "js": ["src/injector.js"],
        "matches": [
            "http://*/*",
            "https://*/*" 
        ]
    }
}